package Demo;

public class Ltts {

	public static void main(String[] args) {
		
		String str = "Auotmation";
		
		//System.out.println(str.length());		
//		for (int i = str.length()-1; i > 0; i--) {
//			System.out.print(str.toCharArray()[i]);
//		}

		String newStr = str.toLowerCase();
		int count;
		for(int i=0;i<newStr.length();i++) {
			
			count = 1;
			
			for(int j=i+1;j<newStr.length();j++) {
				
				if(newStr.toCharArray()[i] == str.toCharArray()[j]) {
					count++;
				}
				 
			}
			
			if(count>1) {
				System.out.println(newStr.toCharArray()[i]);
			}
			
		}
		
	}

}
