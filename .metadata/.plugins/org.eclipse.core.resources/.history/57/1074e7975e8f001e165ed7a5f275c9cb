import java.util.Arrays;

public class DEmoQK {

	public static void main(String[] args) {
		
		int a[] = {6,8,3,5,7};
		int a1[][] = {{1,1},{2,2}};
		
		System.out.println("==> "+a1[0]);
		
		Arrays.sort(a);
	
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		
		}

		System.out.println("Largest Element values in arrya ->  "+(a[a.length-8]));
	}

}
