public class Moverring {
    public static void main(String[] args) {
        String input = "ABCDABEA";
      
        int count;
        
        for(int i = 0; i < input.length(); i++) {
        	
        	count = 1;
        	
        	for (int j = i+1; j < input.length(); j++) {
					
        		if(input.toCharArray()[i] == input.toCharArray()[j] && input.toCharArray()[i] !='0') {
        			
        			input.toCharArray()[j] = '0';
        			count++;
        			
        		}
			}
        	
        	 if(count >= 1 && input.toCharArray()[i] != '0')  
                 System.out.println(input.toCharArray()[i]+" -> "+count);  
        	}      
    }
}

