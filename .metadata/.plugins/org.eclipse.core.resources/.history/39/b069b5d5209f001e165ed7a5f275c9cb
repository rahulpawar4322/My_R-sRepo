public class Moverring {
    public static void main(String[] args) {
        String input = "ABCDABE";
      
        int count;
        
        for(int i = 0; i < input.length(); i++) {
        	
        	count = 1;
        	
        	for (int j = i+1; j < input.length(); j++) {
					
        		if(input.toCharArray()[i] == input.toCharArray()[j]) {
	        			if(input.toCharArray()[j] != '0') {
		        			input.toCharArray()[j] = '0';
		        			
	        			}
	        			count++;
        		}
			}
        	
        	System.out.println(input.toCharArray()[i]+" -> "+count);
        	
		}  
        
    }
}

