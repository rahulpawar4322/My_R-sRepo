import java.util.Arrays;

public class DEmoQK {

	public static void main(String[] args) {
		
		int a[] = {6,8,3,5,7};
		
		Arrays.sort(a);
	
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		
		}

		String s = null;
		System.out.println(s.length());
		System.out.println("Largest Element values in arrya ->  "+(a[a.length-8]));
	}

}
